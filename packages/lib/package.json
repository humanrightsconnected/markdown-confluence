        "@types/node": "^24.9.2",
        "@types/spark-md5": "^3.0.5",
        "@types/yargs": "^17.0.33",
        "cross-env": "^10.1.0",
        "eslint": "^9.38.0",
        "eslint-config-prettier": "^10.1.8",
        "globals": "^16.4.0",
        "jest": "^30.2.0",
